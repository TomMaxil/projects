<!DOCTYPE html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Online SuperMarket System</title>
    <link href="https://fonts.googleapis.com/css?family=Cairo:400,600,700&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:600&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400i,700i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Ubuntu&amp;display=swap" rel="stylesheet">
    <link rel="shortcut icon" type="image/x-icon" href="~/Website/images/favicon.png" />
    <link rel="stylesheet" type="text/css" href="~/Website/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Website/css/animate.min.css">
    <link rel="stylesheet" type="text/css" href="~/Website/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Website/css/nice-select.css">
    <link rel="stylesheet" type="text/css" href="~/Website/css/slick.min.css">
    <link rel="stylesheet" type="text/css" href="~/Website/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Website/css/main-color.css">

</head>
<body class="biolife-body">

    <div id="biof-loading">
        <div class="biof-loading-center">
            <div class="biof-loading-center-absolute">
                <div class="dot dot-one"></div>
                <div class="dot dot-two"></div>
                <div class="dot dot-three"></div>
            </div>
        </div>
    </div>

   

    <header id="header" class="header-area style-01 layout-03">
        <div class="header-top bg-main hidden-xs">
            <div class="container">
                <div class="top-bar left">
                    <ul class="horizontal-menu">
                        <li><a href="#"><i class="fa fa-envelope" aria-hidden="true"></i>Supermarket@Online.com</a></li>
                    </ul>
                </div>
                <div class="top-bar right">
                    <ul class="social-list">
                        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                        <li><a href="#"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
                    </ul>
                    <ul class="horizontal-menu">
                       
                        @if(Context.Session.GetString("Userid") != null){

                            <li><a asp-controller="User" asp-action="Index" class="login-link"><i class="fas fa-user"></i>Profile</a></li>
                            <li><a asp-controller="User" asp-action="Logout" class="login-link"><i class="fas fa-sign-out-alt"></i>Logout</a></li>
                        }
                        else
                        {
                            <li><a asp-controller="User" asp-action="Register" class="login-link"><i class="biolife-icon icon-login"></i>Login/Register</a></li>
                        }
                    </ul>
                </div>

            </div>
        </div>

        <div class="header-middle biolife-sticky-object ">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-md-2 col-md-6 col-xs-6" style="padding-left: 0;">
                        <a href="index-2.html" class="biolife-logo" style="float: right; margin-right: 150px;">
                            <img src="~/Website/images/supermarket-logo.png" alt="supermarket logo" width="155" height="39">
                        </a>
                    </div>
                    <div class="col-lg-6 col-md-7 hidden-sm hidden-xs" style="font-weight: bolder!important; text-align: center!important; font-family: 'Poppins', sans-serif;">
                        <div class="primary-menu">
                            <ul class="menu biolife-menu clone-main-menu clone-primary-menu" id="primary-menu" data-menuname="main menu">
                                @if(Context.Session.GetString("Userid") != null)
                                {
                                    <li class="menu-item"><a asp-controller="Customer" asp-action="Dashboard" >Dashboard</a></li>
                                    <li class="menu-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                                    <li class="menu-item"> <a asp-controller="Home" asp-action="About">About</a></li>
                                    <li class="menu-item"><a asp-controller="Home" asp-action="AllProducts">Product</a> </li>
                                    <li class="menu-item"><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                                }
                                else
                                {
                                    <li class="menu-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                                    <li class="menu-item"> <a asp-controller="Home" asp-action="About">About</a></li>
                                    <li class="menu-item"><a asp-controller="Home" asp-action="AllProducts">Product</a> </li>
                                    <li class="menu-item"><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                                }
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-md-6 col-xs-6">
                        <div class="biolife-cart-info">
                            <div class="mobile-search">
                                <a href="javascript:void(0)" class="open-searchbox"><i class="biolife-icon icon-search"></i></a>
                                <div class="mobile-search-content">
                                    <form action="#" class="form-search" name="mobile-seacrh" method="get">
                                        <a href="#" class="btn-close"><span class="biolife-icon icon-close-menu"></span></a>
                                        <input type="text" name="s" class="input-text" value="" placeholder="Search here...">
                                        <button type="submit" class="btn-submit">go</button>
                                    </form>
                                </div>
                            </div>
                            <div class="minicart-block">
                                <div class="minicart-contain">
                                    <a href="javascript:void(0)" class="link-to">
                                        <span class="icon-qty-combine">
                                            <i class="icon-cart-mini biolife-icon"></i>
                                            <span class="qty">@ViewBag.CartCount</span>
                                        </span>
                                        <span class="title">My Cart -</span>
                                        <span class="sub-total">Rs @ViewBag.CartTotal</span>
                                    </a>

                                    <div class="cart-content">
                                        <div class="cart-inner">
                                            @{
                                                var db = Context.RequestServices.GetService<eCommerceContext>();
                                                var userIdStr = Context.Session.GetString("Userid");

                                                if (userIdStr != null)
                                                {
                                                    var userId = Guid.Parse(userIdStr);
                                                    var customer = db.tbl_customer.FirstOrDefault(c => c.UserId == userId);

                                                    if (customer != null)
                                                    {
                                                        var cart = db.tbl_cart.FirstOrDefault(c => c.CustomerId == customer.id);

                                                        if (cart != null)
                                                        {
                                                            var miniItems = db.tbl_cartitem
                                                            .Where(ci => ci.CartId == cart.CartId)
                                                            .Include(ci => ci.Product)
                                                            .ToList();

                                                            ViewBag.CartCount = miniItems.Sum(i => i.Quantity); 
                                                            ViewBag.CartTotal = miniItems.Sum(i => i.Quantity * i.Product.unitCost);
                                                            <ul class="products">
                                                                @foreach (var item in miniItems)
                                                                {
                                                                    <li>
                                                                        <div class="minicart-item">
                                                                            <div class="thumb">
                                                                                <img src="~/images/@item.Product.image" style="width:60px;height:60px" />
                                                                            </div>

                                                                            <div class="left-info">
                                                                                <h4 class="product-title">@item.Product.ProductName</h4>
                                                                                <p class="price">Qty: @ViewBag.CartCount = 0;</p>
                                                                                <p class="price">Rs @ViewBag.CartTotal = 0;</p>
                                                                            </div>

                                                                           
                                                                        </div>
                                                                    </li>
                                                                }
                                                            </ul>
                                                        }
                                                    }
                                                }
                                            }

                                            <p class="btn-control">
                                                <a asp-controller="Customer" asp-action="Cart" class="btn view-cart">View Cart</a>
                                                <a asp-controller="Checkout" asp-action="Index" class="btn">Checkout</a>
                                            </p>

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="mobile-menu-toggle">
                                <a class="btn-toggle" data-object="open-mobile-menu" href="javascript:void(0)">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="header-bottom hidden-sm hidden-xs">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-md-4 order-lg-2 order-md-2" style="padding-right: 0;">
                        <div class="vertical-menu vertical-category-block" style="margin-top: 5px; margin-right: 80px;">
                            <div class="block-title">

                                <span class="menu-icon">
                                    <span class="line-1"></span>
                                    <span class="line-2"></span>
                                    <span class="line-3"></span>
                                </span>

                                <span class="menu-title">All Categories</span>

                                <span class="angle" data-tgleclass="fa fa-caret-down">
                                    <i class="fa fa-caret-up" aria-hidden="true"></i>
                                </span>
                            </div>

                            <div class="wrap-menu">

                               
                                @await Component.InvokeAsync("CategoryMenu")

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-9 col-md-8 padding-top-2px">
                        <div class="header-search-bar layout-01">
                            <style>
                                .search-modern-big {
                                    width: 105%;
                                    background: #fff;
                                    border-radius: 63px;
                                    border: 5px solid red;
                                    padding: 8px 15px;
                                    display: flex;
                                    align-items: center;
                                    height: 55px; 
                                    box-shadow: 0px 3px 10px rgba(0,0,0,0.05);
                                    margin-left: 90px!important;
                                    
                                }

                                    .search-modern-big input {
                                        width: 90%;
                                        border: none;
                                        outline: none;
                                        font-size: 16px; 
                                        padding-left: 10px;
                                        
                                    }

                                    .search-modern-big button {
                                        width: 60px;
                                        height: 40px;
                                        border: none;
                                        border-radius: 25px;
                                        
                                        color: #fff;
                                        font-size: 18px;
                                        cursor: pointer;
                                        transition: .3s;
                                        margin-right: -11px!important;
                                    }

                                        .search-modern-big button:hover {
                                            background: black;
                                        }
                            </style>
                            <form action="/Products/Search" method="get" class="search-modern-big">
                                <input type="text" name="s" placeholder="Search for oil, milk, biscuits, fruits, tea..." />
                                <button type="submit">
                                    <i class="biolife-icon icon-search"></i>
                                </button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </header>



    @RenderBody()

    @{
        <style>
            .supermarket-footer {
